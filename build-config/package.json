{
  "name": "pomodoro",
  "version": "1.0.0",
  "description": "Pomodoro timer app",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --include-path scss src/scss/app.scss src/css/app.css",
    "sass-watch": "node-sass -w src/scss/app.scss -o src/css/",
    "build": "rimraf dist && NODE_ENV=production webpack -p",
    "start": "webpack-dev-server",
    "dev": "webpack -d --watch"
